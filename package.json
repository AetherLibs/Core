{
  "name": "aethercore",
  "version": "1.1.4",
  "description": "Core",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn tsc",
    "dist": "yarn rimraf ./dist && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AetherLibs/Core.git"
  },
  "author": "The Aether Team <git@ethanlibs.co> (aether.codes)",
  "license": "",
  "bugs": {
    "url": "https://github.com/AetherLibs/Core/issues"
  },
  "homepage": "https://github.com/AetherLibs/Core#readme",
  "packageManager": "yarn@3.2.0",
  "devDependencies": {
    "@aethernet/tsconfig": "^1.1.1",
    "@types/moment": "^2.13.0",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "validator": "^13.7.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/node": "^17.0.21",
    "axios": "^0.26.1",
    "chalk": "^5.0.1",
    "got": "^12.0.2",
    "moment": "^2.29.1"
  },
  "prettier": {
    "printWidth": 200,
    "tabWidth": 4,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "embeddedLanguageFormatting": "auto"
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "dist"
    ],
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  },
  "resolutions": {
    "Test": "portal:/workspace/Test"
  }
}
